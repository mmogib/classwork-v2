{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layout/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@content/*": ["src/content/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@mytypes/*": ["src/types/*"]
    }
  }
}
